{
  "buildCommand": "cd .. && npm install && turbo run build --filter=@tangerine/admin",
  "outputDirectory": "dist",
  "installCommand": "cd .. && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

